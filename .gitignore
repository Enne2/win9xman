# Windows 9x Manager .gitignore

# Disk images
*.img
*.vhd
*.vdi
*.vmdk

# ISO files
*.iso

# Snapshots
snapshots/*
snapshots_win95/*

# Generated/temporary config files
temp_dosbox.conf

# Backup files
*.bak
*~
*.old
*.backup

# Log files
*.log
*.log.*

# Windows system files
win98_drive/*
win95_drive/*

# Python specific
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
env/
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
*.egg-info/
.installed.cfg
*.egg
venv/
.venv/
.env/
.python-version

# Editor specific files
.vscode/
.idea/
*.swp
*.swo
.DS_Store
